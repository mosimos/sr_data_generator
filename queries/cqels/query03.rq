PREFIX ns1: <http://kr.tuwien.ac.at/dhsr/>
SELECT ?stt_id ?del
FROM NAMED <http://kr.tuwien.ac.at/dhsr/>
WHERE {
	STREAM <http://kr.tuwien.ac.at/dhsr/stream> [RANGE 4s]
	{?stt_id ns1:hasDelay ?del.}
	UNION
	{
	STREAM <http://kr.tuwien.ac.at/dhsr/stream> [RANGE 4s]
	{?stt_id ns1:hasArrived ?del.}
	}
	}

